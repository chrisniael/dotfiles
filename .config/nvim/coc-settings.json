{
  "coc.preferences.formatOnType": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSave": true,
  "semanticTokens.enable": true,
  "suggest.completionItemKindLabels": {
    "text": "Text",
    "method": "Method",
    "function": "Function",
    "constructor": "Constructor",
    "field": "Field",
    "variable": "Variable",
    "class": "Class",
    "interface": "Interface",
    "module": "Module",
    "property": "Property",
    "unit": "Unit",
    "value": "Value",
    "enum": "Enum",
    "keyword": "Keyword",
    "snippet": "Snippet",
    "color": "Color",
    "file": "File",
    "reference": "Reference",
    "folder": "Folder",
    "enumMember": "EnumMember",
    "constant": "Constant",
    "struct": "Struct",
    "event": "Event",
    "operator": "Operator",
    "typeParameter": "TypeParameter",
    "default": ""
  },
  "list.insertMappings": {
    "<C-k>": "prompt:removetail",
    "<C-f>": "prompt:right",
    "<C-b>": "prompt:left"
  },
  "list.normalMappings": {
    "<C-o>": "eval: "
  },
  "yank.enableCompletion": false,
  "git.enableGutters": false,
  "clangd.path": "clangd",
  "clangd.enabled": true,
  "clangd.arguments": [
    "--clang-tidy",
    "--completion-style=detailed",
    "--background-index",
    "--pch-storage=memory",
    "--function-arg-placeholders=1"
  ],
  "clangd.disableDiagnostics": false,
  "clangd.fallbackFlags": [],
  "go.goplsPath": "gopls",
  "go.goplsOptions": {
    "semanticTokens": true,
    "usePlaceholders": true
  },
  "rust-analyzer.server.path": "rust-analyzer",
  "python.pythonPath": "python3",
  "Lua.semantic.enable": true,
  "Lua.completion.callSnippet": "Replace",
  "snippets.ultisnips.pythonPrompt": false
}
