{
  "list.previousKeymap": "<C-p>",
  "list.nextKeymap": "<C-n>",
  "list.insertMappings": {
    "<C-k>": "prompt:removetail",
    "<C-f>": "prompt:right",
    "<C-b>": "prompt:left"
  },
  "list.normalMappings": {
    "<C-o>": "eval: "
  },
  "coc.preferences.formatOnType": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSave": true,
  "clangd.enabled": true,
  "clangd.arguments": ["--clang-tidy"],
  "clangd.disableDiagnostics": false,
  "clangd.fallbackFlags": ["-std=c++2a", "-fcoroutines"],
  "yank.enableCompletion": false,
  "rust-analyzer.server.path": "rust-analyzer",
  "python.pythonPath": "python3",
  "go.goplsPath": "gopls",
  "git.enableGutters": false,
  "Lua.semantic.enable": false,
  "clangd.path": "clangd"
}
