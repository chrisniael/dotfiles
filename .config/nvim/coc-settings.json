{
  "coc.preferences.formatOnType": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSave": true,
  "semanticTokens.enable": true,
  "list.insertMappings": {
    "<C-k>": "prompt:removetail",
    "<C-f>": "prompt:right",
    "<C-b>": "prompt:left"
  },
  "list.normalMappings": {
    "<C-o>": "eval: "
  },
  "yank.enableCompletion": false,
  "git.enableGutters": false,
  "clangd.path": "clangd",
  "clangd.enabled": true,
  "clangd.arguments": [
    "--clang-tidy",
    "--completion-style=detailed",
    "--background-index",
    "--pch-storage=memory"
  ],
  "clangd.disableDiagnostics": false,
  "clangd.fallbackFlags": [
    "-std=c++2a",
    "-fcoroutines-ts"
  ],
  "go.goplsOptions": {
    "semanticTokens": true
  },
  "rust-analyzer.server.path": "rust-analyzer",
  "python.pythonPath": "python3",
  "Lua.semantic.enable": false
}
