{
  "languageserver": {
    "ccls": {
      "enable": false,
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "bash": {
      "enable": true,
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  },
  "list.previousKeymap": "<C-p>",
  "list.nextKeymap": "<C-n>",
  "list.insertMappings": {
    "<C-k>": "prompt:removetail",
    "<C-f>": "prompt:right",
    "<C-b>": "prompt:left",
    "<C-v>": "prompt:paste"
  },
  "list.normalMappings": {
    "<C-o>": "command:"
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "cpp",
    "go",
    "html",
    "css",
    "markdown",
    "json",
    "python",
    "yaml",
    "rust"
  ],
  "clangd.enabled": true,
  "clangd.arguments": ["--clang-tidy"],
  "clangd.disableDiagnostics": false,
  "clangd.semanticHighlighting": false,
  "clangd.fallbackFlags": [
    "-std=c++2a",
    "-fcoroutines-ts",
    "-stdlib=libc++",
    "-pthread",
    "-O0",
    "-g",
    "-ggdb",
    "-lc++",
    "-lc++abi"
  ],
  "yank.enableCompletion": false,
  "rust-analyzer.server.path": "rust-analyzer",
  "python.pythonPath": "python3",
  "go.goplsPath": "gopls",
  "go.disable": {
    "workspaceFolders": true
  },
  "git.enableGutters": false
}
