{
  "list.previousKeymap": "<C-p>",
  "list.nextKeymap": "<C-n>",
  "list.insertMappings": {
    "<C-k>": "prompt:removetail",
    "<C-f>": "prompt:right",
    "<C-b>": "prompt:left"
  },
  "list.normalMappings": {
    "<C-o>": "eval: "
  },
  "coc.preferences.formatOnType": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSave": true,
  "clangd.enabled": true,
  "clangd.arguments": ["--clang-tidy"],
  "clangd.disableDiagnostics": false,
  "clangd.fallbackFlags": [
    "-std=c++2a",
    "-fcoroutines-ts",
    "-stdlib=libc++",
    "-pthread",
    "-O0",
    "-g",
    "-ggdb",
    "-lc++",
    "-lc++abi",
    "--include-directory=/usr/include/c++/12.2.1",
    "--include-directory=/usr/include/c++/12.2.1/x86_64-pc-linux-gnu"
  ],
  "yank.enableCompletion": false,
  "rust-analyzer.server.path": "rust-analyzer",
  "python.pythonPath": "python3",
  "go.goplsPath": "gopls",
  "git.enableGutters": false,
  "Lua.semantic.enable": false
}
